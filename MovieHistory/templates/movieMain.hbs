<h1>Movies in Firebase</h1>
{{#each this}}
<section>
  <header class='movie-title'>
    <h1>{{Title}} ({{Year}})</h1>
  </header>
  <img src="{{Poster}}">
  <ul>
    <li>{{Actors}}</li>
    <li>{{Plot}}</li>
    <li>{{Rated}}</li>
    <li>{{Runtime}}</li>
    <li><button class='deletebtn'>x</button></li>
  </ul>
</section>
{{/each}}